# Supabase - Get these from your Supabase project settings
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Discord OAuth - Get these from Discord Developer Portal
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret
NEXT_PUBLIC_DISCORD_REDIRECT_URI=https://your-app.railway.app/api/auth/callback

# Anthropic AI - Get from console.anthropic.com
ANTHROPIC_API_KEY=your_anthropic_api_key

# App URL - Your Railway deployment URL
NEXT_PUBLIC_APP_URL=https://your-app.railway.app
